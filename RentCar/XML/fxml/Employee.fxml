<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EmployeeController">
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <Button fx:id="cars" layoutX="22.0" layoutY="110.0" mnemonicParsing="false" onAction="#cars_button" prefHeight="50.0" prefWidth="270.0" text="Wyświetl samochody">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="customer" layoutX="300.0" layoutY="110.0" mnemonicParsing="false" onAction="#customer_button" prefHeight="50.0" prefWidth="270.0" text="Obsługa klientów">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="history" layoutX="22.0" layoutY="194.0" mnemonicParsing="false" onAction="#history_button" prefHeight="50.0" prefWidth="270.0" text="Wyświetl historię">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="rental" layoutX="300.0" layoutY="194.0" mnemonicParsing="false" onAction="#rental_button" prefHeight="50.0" prefWidth="270.0" text="Obsługa wypożyczeń">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="password" layoutX="22.0" layoutY="279.0" mnemonicParsing="false" onAction="#password_button" prefHeight="50.0" prefWidth="270.0" text="Zmień hasło">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="logout" layoutX="300.0" layoutY="279.0" mnemonicParsing="false" onAction="#logout_button" prefHeight="50.0" prefWidth="270.0" text="Wyloguj">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Text fx:id="welcome" layoutY="60.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER" wrappingWidth="600.0">
               <font>
                  <Font size="36.0" />
               </font></Text>
         </children>
      </Pane>
   </children>
</StackPane>
